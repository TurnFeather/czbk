<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hibernate.test.orphan">

<!-- 建立到表tbl_accident_reason_to_stand的映射 -->
	<class name="cn.edu.ncist.dataAnalyze.realtime.vo.ReasonToStandModel"  table="tbl_accident_reason_to_stand">
		<id name="id" column="id" type="string">
			<generator class="native"></generator>
		</id>
		<property name="acdId" column="acdId" type="string"></property>
		<property name="stdReasonId" column="stdReasonId" type="int"></property>
		<property name="degree" column="degree" type="string"></property>
	</class>
</hibernate-mapping>